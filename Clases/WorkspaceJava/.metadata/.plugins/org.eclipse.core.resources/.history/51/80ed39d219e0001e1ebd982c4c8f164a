package com.krakedev;

public class TestAuto {

	public static void main(String[] args) {
		// Declar una variable de tipo Auto
		Auto auto1 = new Auto();
		Auto auto2 = new Auto();
		
		//modificar los valores de las varables
		auto1.Marca = "Toyota";
		auto1.anio = 2010;
		auto1.precio = 15.000;
		
		auto2.Marca = "Chevrolet";
		auto2.anio = 1995;
		auto2.precio = 12.000;
	}

}
